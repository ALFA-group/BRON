{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"CarTactic",
    "description":"A link between an analytic and a tactic",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The analytic identifier in BRON.",
         "type":"string",
         "pattern": "car/CAR-\\d{4}-\\d{2}-\\d{3}"
      },
      "_to":{
         "description":"The tactic identifier in BRON.",
         "type":"string",
         "pattern": "tactic/TA\\d{4}"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }
