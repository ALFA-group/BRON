{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"CweCwe_detection",
    "description":"A link between a weakness and a weakness detection",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The weakness identifier in BRON.",
         "type":"string",
         "pattern": "cwe/CWE-\\d{1,4}"
      },
      "_to":{
         "description":"The weakness detection identifier in BRON.",
         "type":"string",
         "pattern": "cwe_detection/CWE-\\d{1,4}"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }