{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"TechniqueTechnique_detection_strategy",
    "description":"A link between a technique and a detection strategy",
    "type":"object",
    "properties":{
      "_to":{
         "description":"The detection strategy identifier in BRON.",
         "type":"string",
         "pattern": "technique_detection_strategy/DET\\d{4}"
      },
      "_from":{
         "description":"The technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }