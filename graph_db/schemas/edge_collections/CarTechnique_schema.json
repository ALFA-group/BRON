{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"CarTechnique",
    "description":"A link between an analytic and a technique",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The analytic identifier in BRON.",
         "type":"string",
         "pattern": "car/CAR-\\d{4}-\\d{2}-\\d{3}"
      },
      "_to":{
         "description":"The technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }
