{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"CarD3fend_mitigation",
    "description":"A link between an analytic and a defensive mitigation",
    "type":"object",
    "properties":{
      "_to":{
         "description":"The defensive mitigation identifier in BRON.",
         "type":"string",
         "pattern": "d3fend_mitigation/D3[A-Z]?-[A-Z]+"
      },
      "_from":{
         "description":"The analytic identifier in BRON.",
         "type":"string",
         "pattern": "car/CAR-\\d{4}-\\d{2}-\\d{3}"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }
