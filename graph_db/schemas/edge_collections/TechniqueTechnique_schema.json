{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"TechniqueTechnique",
    "description":"A link between a technique and a sub technique",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      },
      "_to":{
         "description":"The sub technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }