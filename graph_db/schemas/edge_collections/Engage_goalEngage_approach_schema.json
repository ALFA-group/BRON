{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"Engage_goalEngage_approach",
    "description":"A link between a defensive approach and a defensive activity",
    "type":"object",
    "properties":{
      "_to":{
         "description":"The approach identifier in BRON.",
         "type":"string",
         "pattern": "engage_approach/[ES]AP\\d{4}"
      },
      "_from":{
         "description":"The goal identifier in BRON.",
         "type":"string",
         "pattern": "engage_goal/[ES]GO\\d{4}"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }