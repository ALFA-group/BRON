{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"Atlas_tacticTactic",
    "description":"A link between an ATLAS tactic and an ATT&CK Tactic",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The atlas tactic identifier in BRON.",
         "type":"string",
         "pattern": "atlas_tactic/AML.TA\\d{4}"
      },
      "_to":{
         "description":"The tactit identifier in BRON.",
         "type":"string",
         "pattern": "tactic/TA\\d{4}"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }