{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"TechniqueEngage_activity",
    "description":"A link between a technique and a defensive activity",
    "type":"object",
    "properties":{
      "_to":{
         "description":"The activity identifier in BRON.",
         "type":"string",
         "pattern": "engage_activity/[ES]AC\\d{4}"
      },
      "_from":{
         "description":"The technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }