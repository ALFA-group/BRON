{
    "$schema":"http://json-schema.org/draft-07/schema#",
    "title":"TacticTechnique",
    "description":"A link between a tactic and a technique",
    "type":"object",
    "properties":{
      "_from":{
         "description":"The tactic identifier in BRON.",
         "type":"string",
         "pattern": "tactic/TA\\d{4}"
      },
      "_to":{
         "description":"The technique identifier in BRON.",
         "type":"string",
         "pattern": "technique/(T\\d{4}(\\.\\d{3})?)"
      }
 },
    "required":[
      "_from",
      "_to"
      ]
  }